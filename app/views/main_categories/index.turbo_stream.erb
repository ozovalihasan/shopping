<%= turbo_stream.replace "user-account" do %>
    <%= render component("layout/user_account", current_user: current_user) %>
<% end %>

<%= turbo_stream.replace "main-parts" do %>
    <%= render component("shared/main_page", main_categories: @main_categories, products: @products) %>
<% end %>

<%= turbo_stream.replace "cart" do %>
    <%= render component("carts/navbar", cart: @cart) %>
<% end %>